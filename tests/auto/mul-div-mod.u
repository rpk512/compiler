extern void print_line(string s);
extern void exit(int n);

void assert(bool condition) {
    if (!condition) {
        print_line("FAIL");
        exit(1);
    }
}

void main() {
    var a int;
    var b int;

    a = 32 * -64;
    assert(a == -2048);

    a = 1 * 2 * 3;
    b = 4 * 5 * 6;
    assert(a * b == 720);

    a = 64 / -32;
    assert(a == -2);

    a = 100 / 2 / 5;
    b = -80 / 8 / -1;
    assert(a / b == 1);

    a = 46 % 9;
    assert(a == 1);

    a = 456 % 123 % 4;
    b = 63 % 11 % 3;
    assert(a % b == 1);

    print_line("PASS");
}
