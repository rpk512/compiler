import "test";

void main() {
    var a int[10];
    var p int*[10];
    var i int;

    i = 0;
    while (i < 10) {
        p[i] = &a[i];
        *p[i] = i + 1;
        i = i + 1;
    }

    i = 0;
    while (i < 10) {
        test:assert(a[i] == i + 1);
        i = i + 1;
    }

    test:pass();
}
