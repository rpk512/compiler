extern void print64(int n);
extern void print_line(string s);
extern void exit(int n);

void assert(bool condition) {
    if (!condition) {
        print_line("FAIL");
        exit(1);
    }
}

void test1(int* ptr) {
    test2(&ptr);
}

void test2(int** ptrptr) {
    **ptrptr = 12345;
}

void main() {
    int i;

    i = 0;
    test1(&i);
    assert(i == 12345);

    int* ptr;
    int** ptrptr;

    ptr = &i;
    assert(*ptr == 12345);

    *ptr = 0;
    assert(i == 0);

    ptrptr = &ptr;
    **ptrptr = 54321;
    assert(i == 54321);

    int j;
    j = 0;
    j = *ptr;
    assert(j == 54321);

    j = 1024;
    *ptrptr = &j;
    assert(**ptrptr == 1024);

    print_line("PASS");
}
