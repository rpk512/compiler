import "test";

void main() {
    var a int;
    var b int;

    a = 32 * -64;
    test:assert(a == -2048);

    a = 1 * 2 * 3;
    b = 4 * 5 * 6;
    test:assert(a * b == 720);

    a = 64 / -32;
    test:assert(a == -2);

    a = 100 / 2 / 5;
    b = -80 / 8 / -1;
    test:assert(a / b == 1);

    a = 46 % 9;
    test:assert(a == 1);

    a = 456 % 123 % 4;
    b = 63 % 11 % 3;
    test:assert(a % b == 1);

    test:pass();
}
