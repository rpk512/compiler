import "test";

void main() {
    var i int;
    var n int;

    n = 0;
    i = 1;
    while (i <= 5) {
        i = i + 1;
        n = n + 1;
    }

    while (i >= 1) {
        i = i - 1;
        n = n + 1;
    }

    if (i == 0 && n == 11) {
        test:pass();
    } else {
        test:fail();
    }
}
